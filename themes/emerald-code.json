{
    "name": "Emerald Code",
    "type": "dark",
    "colors": {
        "editor.background": "#0f1b0f",
        "editor.foreground": "#b8e994",
        "activityBar.background": "#0a140a",
        "activityBar.foreground": "#00cc66",
        "sideBar.background": "#0a140a",
        "sideBar.foreground": "#b8e994",
        "statusBar.background": "#0a140a",
        "statusBar.foreground": "#00cc66",
        "titleBar.activeBackground": "#0a140a",
        "titleBar.activeForeground": "#00cc66",
        "tab.activeBackground": "#0f1b0f",
        "tab.activeForeground": "#00cc66",
        "tab.inactiveBackground": "#0a140a",
        "tab.inactiveForeground": "#4a6b4a",
        "editorGroupHeader.tabsBackground": "#0a140a",
        "input.background": "#0f1b0f",
        "input.foreground": "#b8e994",
        "input.border": "#00ff99",
        "dropdown.background": "#0f1b0f",
        "dropdown.foreground": "#b8e994",
        "list.activeSelectionBackground": "#00ff9933",
        "list.activeSelectionForeground": "#00cc66",
        "list.hoverBackground": "#00ff9919",
        "editor.lineHighlightBackground": "#00ff990d",
        "editor.selectionBackground": "#00ff9933",
        "editorCursor.foreground": "#00cc66",
        "editorWhitespace.foreground": "#4a6b4a",
        "editorIndentGuide.background": "#4a6b4a33",
        "editor.findMatchBackground": "#00ff9933",
        "editor.findMatchHighlightBackground": "#00cc6633"
    },
    "tokenColors": [
        {
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#4a6b4a",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["string", "string.quoted"],
            "settings": {
                "foreground": "#66ffcc"
            }
        },
        {
            "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
            "settings": {
                "foreground": "#00cc66"
            }
        },
        {
            "scope": ["variable", "variable.other"],
            "settings": {
                "foreground": "#99ff66"
            }
        },
        {
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#00ff99"
            }
        },
        {
            "scope": ["entity.name.function", "support.function"],
            "settings": {
                "foreground": "#33cc33"
            }
        },
        {
            "scope": ["entity.name.type", "support.type", "support.class"],
            "settings": {
                "foreground": "#66ffcc"
            }
        },
        {
            "scope": ["string.regexp", "constant.character.escape"],
            "settings": {
                "foreground": "#00cc66"
            }
        },
        {
            "scope": ["punctuation.definition", "punctuation.separator", "punctuation.terminator"],
            "settings": {
                "foreground": "#b8e994"
            }
        },
        {
            "scope": ["entity.name.tag", "punctuation.definition.tag"],
            "settings": {
                "foreground": "#00ff99"
            }
        }
    ]
} 